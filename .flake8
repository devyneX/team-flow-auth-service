[flake8]
ignore = C101,D403,E126,I100,I201,W503,W504
exclude = [
    **/migrations/*,
    .git,
    .mypy_cache,
    __pycache__,
    venv]
filename = *.py
accept-encodings = utf-8
inline-quotes = single
max-complexity = 12
max-linenumber = 500
max-line-length = 119
multiline-quotes = double
per-file-ignores =
    src/manage.py: Q000